  },
  "netty": {
    "fileServerEnabled": true,
    "sendExceptionEnabled": true,
    "ipForwarding": false,
    "logLevel": "DEBUG"
  },
  "launcher": {
    "guardType": "no",
    "compress": true,
    "deleteTempFiles": true,
    "certificatePinning": false,
    "encryptRuntime": false
