        "expireSeconds": 3600,
        "uuidColumn": "uuid",
        "usernameColumn": "username",
        "accessTokenColumn": "accesstoken",
        "passwordColumn": "password",
        "serverIDColumn": "serverid",
        "hardwareIdColumn": "hwidId",
        "tableHWID": "hwids",
        "tableHWIDLog": "hwidLog",
        "table": "users",
        "passwordVerifier": {
          "cost": 10,
          "type": "bcrypt"
        },
        "type": "sql"
      },
      "textureProvider": {
        "skinURL": "https://crafatar.com/skins/%username%",
        "cloakURL": "https://crafatar.com/cloaks/%username%",
        "type": "request"
      },
      "displayName": "EconomyAuth",
      "visible": true
    }
  },
  "permissionsHandler": {
    "holder": {
      "driverClass": "org.postgresql.Driver",
      "jdbcUrl": "jdbc:postgresql://pgbouncer-master:5432/econ",
      "username": "game",
      "password": "gamepass",
      "hikariMaxLifetime": 1800000,
      "initializeAtStart": true
    },
    "table": "user_permissions",
    "uuidColumn": "uuid",
    "permissionColumn": "name",
    "type": "sql"
  },
  "hwid": {
    "handler": {
      "type": "memory"
